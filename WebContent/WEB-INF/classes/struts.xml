<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <constant name="struts.devMode" value="true" />
    <constant name="struts.tags.escapeHtml" value="true" />

    <package name="default" namespace="/" extends="struts-default">

        <action name="exibirDadosCadastrais" class="br.com.bradesco.previdencianc.action.AporteAction" method="execute">
            <result name="input">/aporteDadosCadastrais.jsp</result>
        </action>
        
        <action name="confirmacaoDadosAporte" class="br.com.bradesco.previdencianc.action.AporteAction" method="confirmacaoDadosAporte">
            <result name="success">/sucesso.jsp</result>
            <result name="input">/aporteDadosCadastrais.jsp</result>
        </action>

        </package>
</struts>